<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frontend.media.mapper.ContentStatisticalMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.frontend.media.entity.ContentStatistical">
        <id column="id" property="id" />
        <result column="client_listen_id" property="clientListenId" />
        <result column="res_content_id" property="resContentId" />
        <result column="use_time" property="useTime" />
        <result column="content_time" property="contentTime" />
        <result column="ip" property="ip" />
        <result column="create_time" property="createTime" />
        <result column="status" property="status" />
    </resultMap>

</mapper>
