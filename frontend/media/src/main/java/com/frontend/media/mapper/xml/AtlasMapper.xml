<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frontend.media.mapper.AtlasMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.frontend.media.entity.Atlas">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="category_id" property="categoryId" />
        <result column="img_count" property="imgCount" />
        <result column="cover_img_ids" property="coverImgIds" />
        <result column="stat" property="stat" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="client_listen_id" property="clientListenId" />
    </resultMap>

</mapper>
