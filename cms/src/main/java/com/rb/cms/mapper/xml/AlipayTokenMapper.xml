<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rb.cms.mapper.AlipayTokenMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rb.cms.entity.AlipayToken">
        <id column="client_listen_id" property="clientListenId" />
        <result column="authCode" property="authCode" />
        <result column="user_id" property="userId" />
        <result column="access_token" property="accessToken" />
        <result column="expires_in" property="expiresIn" />
        <result column="refresh_token" property="refreshToken" />
        <result column="re_expires_in" property="reExpiresIn" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="expire_time" property="expireTime" />
    </resultMap>

</mapper>
