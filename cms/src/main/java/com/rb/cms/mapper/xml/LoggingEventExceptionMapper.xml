<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rb.cms.mapper.LoggingEventExceptionMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rb.cms.entity.LoggingEventException">
        <id column="event_id" property="eventId" />
        <result column="i" property="i" />
        <result column="trace_line" property="traceLine" />
    </resultMap>

</mapper>
