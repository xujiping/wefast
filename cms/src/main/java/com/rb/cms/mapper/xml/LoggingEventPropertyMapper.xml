<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rb.cms.mapper.LoggingEventPropertyMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rb.cms.entity.LoggingEventProperty">
        <id column="event_id" property="eventId" />
        <result column="mapped_key" property="mappedKey" />
        <result column="mapped_value" property="mappedValue" />
    </resultMap>

</mapper>
